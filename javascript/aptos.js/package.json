{
  "name": "@switchboard-xyz/aptos.js",
  "description": "API wrapper for intergating with the Switchboardv2 program on Aptos",
  "version": "2.6.6",
  "license": "MIT",
  "files": [
    "lib",
    "src",
    "sbv2-aptos.ts",
    "package.json"
  ],
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/switchboard-xyz/sbv2-aptos.git",
    "directory": "packages/aptos.js"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "build": "shx rm -rf lib || true; tsc -p tsconfig.cjs.json && tsc",
    "watch": "tsc -p tsconfig.cjs.json --watch",
    "test": "node ./node_modules/mocha/bin/mocha --loader=ts-node/esm --extension ts --timeout 10000 ./tests/*.tests.ts",
    "prepack": "npm run lint && npm run build",
    "lint": "gts lint ./src ./tests",
    "fix": "gts fix ./src ./tests",
    "clean": "gts clean"
  },
  "dependencies": {
    "@switchboard-xyz/common": "^2.1.33",
    "aptos": "^1.6.0",
    "big.js": "^6.2.1",
    "bn.js": "^5.2.1",
    "chalk": "^5.0.1",
    "js-sha3": "^0.8.0",
    "node-fetch": "^2.1.6"
  },
  "devDependencies": {
    "@switchboard-xyz/eslint-config": "latest",
    "@types/big.js": "^6.1.6",
    "@types/bn.js": "^5.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.6.4",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.11",
    "eslint": "^8.35.0",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "shx": "^0.3.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
